{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/jobSchema.json",
	"type": "object",
	"properties": {
		"title": {
			"type": "string",
			"minLength": 1,
			"maxLength": 50
		},
		"salary": {
			"type": "integer",
			"minLength": 1,
			"maxLength": 20
		},
		"equity": {
			"type": "string"
		},
		"companyHandle": {
			"type": "string",
			"minLength": 1,
			"maxLength": 50
		}
	},
	"additionalProperties": false,
	"required": ["title", "salary", "equity", "companyHandle"]
}
